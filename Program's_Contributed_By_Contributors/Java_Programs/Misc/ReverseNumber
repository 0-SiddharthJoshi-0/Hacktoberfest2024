 /*
 Example of This Program Does .
 
 
 input : 956
 output : 659
 */
 
 
 
 
 
 import java.util.*;
 
 
public class ResverseNumber
{
	public static void main(String[] args) {
Scanner sc = new Scanner(System.in);
		  System.out.print("Enter Integer To Resvere it : ");
		  int n=sc.nextInt();
		  sc.close();
		  
		  
		  int result= 0;
		  
           
          while(n!=0){
            int  remainder= n%10;
            result =result*10 +remainder;
            n=n/10;
            
          }
            System.out.println ("Your result is  "+ result);	}
}
